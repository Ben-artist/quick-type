{
  "name": "quick-type-backend",
  "version": "1.0.0",
  "description": "Node.js Express backend for Quick Type - URL to TypeScript type generator",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postbuild": "echo 'Build completed successfully!'"
  },
  
  "keywords": [
    "typescript",
    "quicktype",
    "express",
    "api",
    "type-generation"
  ],
  "author": "Quick Type Team",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@types/shelljs": "^0.8.17",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "quicktype-core": "^23.0.0",
    "shelljs": "^0.10.0"
  }
}
